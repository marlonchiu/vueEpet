bg-img($url)
  display block
  width 100%
  height 35px
  padding-top 5px
  vertical-align middle
  background url('./img_head/head_icon'+$url +'.png') 50% 0 no-repeat
  background-size auto 100%


number_style()
  background-color white
  color red
  border-radius 5px
  padding-left 5px
  padding-right 5px
  margin-left 5px
  margin-right 5px
  font-weight 400
